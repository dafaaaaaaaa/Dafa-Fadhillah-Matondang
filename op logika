package dafa;

public class operatorlogika {
    public static void main(String[] args) {
        boolean c = true;
        boolean d = false;
        boolean e;

        // &&(dan)
        e = c && d;
        System.out.println("true && false =" + e);

        //  || (atau)
        e = c || d;
        System.out.println("true || false = " + e);

        // ! (bukan)
        System.out.println("false, !true = " + !c);

    }
}
